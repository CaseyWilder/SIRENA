<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="payroll_dashboard_todo" model="payroll.dashboard">
        <field name="sequence">1</field>
        <field name="type">todo</field>
    </record>
    <record id="payroll_dashboard_employees" model="payroll.dashboard">
        <field name="sequence">2</field>
        <field name="type">employees</field>
    </record>
    <record id="payroll_dashboard_tbd" model="payroll.dashboard">
        <field name="sequence">3</field>
        <field name="type">tbd</field>
    </record>
    <record id="payroll_dashboard_hours" model="payroll.dashboard">
        <field name="sequence">4</field>
        <field name="type">hours</field>
    </record>
    <record id="payroll_dashboard_gross" model="payroll.dashboard">
        <field name="sequence">5</field>
        <field name="type">gross</field>
    </record>
    <record id="payroll_dashboard_cost" model="payroll.dashboard">
        <field name="sequence">6</field>
        <field name="type">cost</field>
    </record>

    <!-- Function to set default deductions for company, which will be shown in Deduction Chart in Payroll Dashboard -->
    <function model="res.company" name="set_default_deductions"/>
</odoo>
