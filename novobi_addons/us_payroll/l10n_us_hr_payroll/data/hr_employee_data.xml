    </record>
    <record id="ethnicity_alaskan_native" model="hr.employee.ethnicity">
        <field name="name">Alaskan Native</field>
    </record>
    <record id="ethnicity_black_american" model="hr.employee.ethnicity">
        <field name="name">Black American</field>
    </record>
    <record id="ethnicity_african_american" model="hr.employee.ethnicity">
        <field name="name">African American</field>
    </record>
    <record id="ethnicity_asian" model="hr.employee.ethnicity">
        <field name="name">Asian</field>
    </record>
    <record id="ethnicity_hispanic" model="hr.employee.ethnicity">
        <field name="name">Hispanic</field>
    </record>
    <record id="ethnicity_latino" model="hr.employee.ethnicity">
        <field name="name">Latino</field>
    </record>
    <record id="ethnicity_native_hawaiian" model="hr.employee.ethnicity">
